{
  "name": "ai-search-scripts",
  "version": "1.0.0",
  "description": "Data ingestion and embedding scripts for AI-powered search engine",
  "scripts": {
    "ingest": "ts-node src/ingest.ts",
    "ingest:sample": "ts-node src/ingest.ts --sample",
    "ingest-data": "ts-node src/ingestData.ts",
    "ingest:newsapi": "ts-node src/ingestData.ts newsapi",
    "ingest:urls": "ts-node src/ingestData.ts urls",
    "ingest:rss": "ts-node src/ingestData.ts rss",
